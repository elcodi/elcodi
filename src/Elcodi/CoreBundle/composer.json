{
    "name": "elcodi/core-bundle",
    "description": "Elcodi Core component",
    "keywords": ["elcodi", "core", "ecommerce"],
    "homepage": "https://github.com/elcodi/CoreBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "zimage@tiscali.it"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/elcodi/CoreBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/common": "~2.4",
        "doctrine/orm": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "symfony/config": "~2.4",
        "symfony/form": "~2.4",
        "symfony/options-resolver": "~2.4",
        "symfony/http-foundation": "~2.4"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.4",
        "symfony/routing": "~2.4",
        "symfony/console": "~2.4",
        "doctrine/data-fixtures": "v1.0.0",
        "phpunit/phpunit": ">=4.0.0",
        "elcodi/elcodi": "0.1.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Elcodi\\CoreBundle": ""
        }
    },
    "target-dir": "Elcodi/Components/CoreBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}