{
    "name": "elcodi/media-bundle",
    "description": "Elcodi Media component",
    "keywords": ["elcodi", "media", "ecommerce"],
    "homepage": "https://github.com/elcodi/MediaBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "zimage@tiscali.it"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/elcodi/ProductBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/common": "~2.4",
        "doctrine/orm": "~2.4",
        "symfony/config": "~2.4",
        "symfony/security": "~2.4",
        "symfony/event": "~2.4",
        "symfony/form": "~2.4",
        "symfony/options-resolver": "~2.4",
        "symfony/validator": "~2.4",
        "symfony/routing": "~2.4",
        "symfony/http-foundation": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "mmoreram/controller-extra-bundle": "~1",

        "elcodi/core-bundle": "0.1.*@dev"
    },
    "require-dev": {
        "doctrine/data-fixtures": "v1.0.0",
        "phpunit/phpunit": ">=4.0.0",
        "elcodi/elcodi": "0.1.*@dev"
    },
    "autoload": {
        "psr-0": { "Elcodi\\ProductBundle": "" }
    },
    "target-dir": "Elcodi/Components/ProductBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}