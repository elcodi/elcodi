{
    "name": "elcodi/plugin",
    "description": "Elcodi Plugin Component",
    "keywords": [
        "elcodi",
        "plugin",
        "ecommerce"
    ],
    "homepage": "https://github.com/elcodi/Plugin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "zimage@tiscali.it"
        },
        {
            "name": "Elcodi Community",
            "homepage": "https://github.com/elcodi/Plugin/contributors"
        }
    ],
    "require": {
        "php": "^5.4|^7.0",
        "doctrine/common": "^2.5",

        "symfony/event-dispatcher": "^2.7|^3.0",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/expression-language": "^2.7|^3.0",
        "symfony/http-kernel": "^2.7|^3.0",
        "symfony/console": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0",
        "twig/twig": "^1.23",

        "elcodi/configuration": "^2.0",
        "elcodi/core": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.19"
    },
    "autoload": {
        "exclude-from-classmap": [
            "/Tests/"
        ],
        "psr-4": {
            "Elcodi\\Component\\Plugin\\": ""
        }
    },
    "suggest": {
        "symfony/dependency-injection": "For EventDispatcher HookSystem adapter"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
