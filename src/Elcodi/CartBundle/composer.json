{
    "name": "elcodi/cart-bundle",
    "description": "Elcodi Cart component",
    "keywords": ["elcodi", "cart", "ecommerce"],
    "homepage": "https://github.com/elcodi/CartBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "zimage@tiscali.it"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/elcodi/CartBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/common": "~2.4",
        "doctrine/orm": "~2.4",
        "symfony/config": "~2.4",
        "symfony/event-dispatcher": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "symfony/expression-language": "~2.4",
        "symfony/http-kernel": "~2.4",
        "symfony/http-foundation": "~2.4",
        "sebastian/money": "~1.3",

        "elcodi/core-bundle": "0.1.*@dev",
        "elcodi/user-bundle": "0.1.*@dev",
        "elcodi/product-bundle": "0.1.*@dev"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.4",
        "symfony/finder": "~2.4",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "phpunit/phpunit": ">=4.0.0"
    },
    "autoload": {
        "psr-4": {
            "Elcodi\\CartBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
